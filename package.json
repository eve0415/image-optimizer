{
    "name": "image-optimizer",
    "type": "module",
    "scripts": {
        "build": "./build.js",
        "dev": "nodemon --watch src -e ts --exec \"yarn build && yarn node -r dotenv/config --enable-source-maps ./out/index.js\"",
        "lint": "prettier --write --ignore-path .gitignore \"./**/*.{json,js,ts}\"",
        "clean": "rimraf out",
        "sdk": "yarn dlx @yarnpkg/sdks"
    },
    "packageManager": "yarn@3.2.1",
    "dependencies": {
        "@fastify/accepts": "next",
        "@fastify/cors": "next",
        "@fastify/type-provider-typebox": "^0.1.0-beta.1",
        "@sinclair/typebox": "^0.23.5",
        "axios": "^0.27.2",
        "axios-extensions": "^3.1.3",
        "fastify": "next",
        "sharp": "^0.30.6"
    },
    "devDependencies": {
        "@types/node": "^17.0.39",
        "@types/rimraf": "^3.0.2",
        "@types/sharp": "^0.30.2",
        "dotenv": "^16.0.1",
        "esbuild": "^0.14.42",
        "esbuild-node-externals": "^1.4.1",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.3"
    }
}
